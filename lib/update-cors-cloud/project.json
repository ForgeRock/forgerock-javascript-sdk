{
  "root": "lib/update-cors-cloud",
  "sourceRoot": "lib/update-cors-cloud/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "ncc build lib/update-cors-cloud/src/index.ts -o dist/lib/update-cors-cloud"
      }
    },
    "serve": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": ["{options.outputPath}"],
      "options": {
        "command": "ncc run dist/lib/update-cors-cloud/index.js"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["lib/update-cors-cloud/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/lib/update-cors-cloud"],
      "options": {
        "jestConfig": "lib/update-cors-cloud/jest.config.ts",
        "passWithNoTests": true,
        "codeCoverage": true
      }
    }
  },
  "tags": []
}
