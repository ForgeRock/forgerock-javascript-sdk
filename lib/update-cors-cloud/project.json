{
  "root": "lib/update-cors-cloud",
  "sourceRoot": "lib/update-cors-cloud/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist//lib/update-cors-cloud",
        "main": "lib/update-cors-cloud/src/index.ts",
        "tsConfig": "lib/update-cors-cloud/tsconfig.lib.json",
        "assets": ["lib/update-cors-cloud/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["lib/update-cors-cloud/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/lib/update-cors-cloud"],
      "options": {
        "jestConfig": "lib/update-cors-cloud/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
