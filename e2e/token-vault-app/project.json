{
  "name": "token-vault-app",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/token-vault-app/src",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "dependsOn": ["^build", "token-vault-app:build-interceptor"],
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/e2e/token-vault-app"
      ],
      "defaultConfiguration": "production",
      "options": {
        "emptyOutDir": false,
        "outDir": "../../dist/e2e/token-vault-app"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "build-interceptor": {
      "dependsOn": ["^build"],
      "inputs": [
        "default",
        "^default",
        {
          "externalDependencies": ["vite"]
        }
      ],
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/e2e/token-vault-app"
      ],
      "options": {
        "emptyOutDir": false,
        "config": "./vite.interceptor.config.ts",
        "outDir": "./public"
      }
    },
    "serve": {
      "dependsOn": ["token-vault-app:build"],
      "defaultConfiguration": "development",
      "options": {
        "host": "localhost",
        "port": 5823
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "lint": {
      "options": {
        "fix": true,
        "ignore-path": ".eslintignore",
        "args": ["**/*.ts"]
      }
    }
  }
}
