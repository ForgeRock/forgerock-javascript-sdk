{
  "name": "autoscript-apps",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/autoscript-apps/src",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "dependsOn": ["^build", "^tsc"],
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/e2e/autoscript-apps"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "watch": true
        },
        "production": {
          "outputPath": "dist/e2e/autoscript-apps",
          "optimized": true,
          "watch": false
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@nx/vite:dev-server",
      "dependsOn": ["^build"],
      "defaultConfiguration": "production",
      "options": {
        "buildTarget": "autoscript-apps:build",
        "port": 8443,
        "host": "sdkapp.example.com"
      },
      "configurations": {
        "production": {
          "buildTarget": "autoscript-apps:build",
          "open": false,
          "hmr": false
        },
        "development": {
          "buildTarget": "autoscript-apps:build:development",
          "open": true,
          "hmr": true
        }
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "autoscript-apps:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "autoscript-apps:build:development"
        },
        "production": {
          "buildTarget": "autoscript-apps:build:production"
        }
      }
    }
  }
}
