{
  "root": "e2e/javascript-sdk-app",
  "sourceRoot": "e2e/javascript-sdk-app/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@ns3/nx-jest-playwright:jest-playwright",
      "options": {
        "jestConfig": "e2e/javascript-sdk-app/jest.config.js",
        "devServerTarget": "javascript-sdk-app:serve"
      },
      "configurations": {
        "live": {
          "devServerTarget": ""
        }
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/javascript-sdk-app/**/*.{js,ts}"],
        "ignorePath": ".gitignore"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["javascript-sdk-app"]
}
