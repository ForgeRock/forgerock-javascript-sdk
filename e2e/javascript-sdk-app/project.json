{
  "root": "e2e/javascript-sdk-app",
  "sourceRoot": "e2e/javascript-sdk-app/src",
  "projectType": "application",
  "targets": {
    "e2e": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          {
            "command": "concurrently -s 'first' --kill-others npm:start:server npm:playwright-test"
          }
        ]
      },
      "configurations": {
        "live": {
          "commands": [
            {
              "command": "npm run start:server:live",
              "description": "run api setup"
            },
            {
              "command": "playwright test",
              "description": "run e2e setup"
            }
          ]
        },
        "parallel": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["e2e/javascript-sdk-app/**/*.{js,ts}"],
        "ignorePath": ".gitignore"
      }
    }
  },
  "tags": [],
  "implicitDependencies": ["javascript-sdk-app"]
}
