{
  "name": "token-vault-proxy",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/token-vault-proxy/src",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "dependsOn": ["javascript-sdk:build", "token-vault:build", "^build"],
      "inputs": ["default", "^default"],
      "outputs": ["{projectRoot}/dist"],
      "defaultConfiguration": "production",
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "dependsOn": ["build"],
      "defaultConfiguration": "development",
      "options": {
        "host": "localhost",
        "port": 5833
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "lint": {
      "options": {
        "fix": true,
        "ignore-path": ".eslintignore",
        "args": ["**/*.ts"]
      }
    }
  }
}
