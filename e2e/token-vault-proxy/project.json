{
  "name": "token-vault-proxy",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "e2e/token-vault-proxy/src",
  "tags": ["scope:app"],
  "targets": {
    "build": {
      "inputs": ["default", "^default"],
      "outputs": [
        "{projectRoot}/{options.outDir}",
        "{workspaceRoot}/dist/e2e/token-vault-proxy"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outDir": "../../dist/e2e/token-vault-proxy"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "defaultConfiguration": "development",
      "options": {
        "host": "localhost",
        "port": 5833
      },
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "preview": {
      "defaultConfiguration": "development",
      "configurations": {
        "development": {},
        "production": {}
      }
    },
    "lint": {
      "options": {
        "args": ["./src/**/*.ts"],
        "ignore-path": ".eslintignore"
      }
    }
  }
}
