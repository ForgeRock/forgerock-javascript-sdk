{
  "extends": ["../../.eslintrc.json", "plugin:react-hooks/recommended"],
  "ignorePatterns": ["!**/*"],
  "overrides": [
    {
      "files": ["*.ts", "*.tsx", "*.js", "*.jsx"],
      "plugins": ["react", "react-hooks"],
      "rules": {
        "no-debugger": 0,
        "no-unused-vars": [
          2,
          {
            "varsIgnorePattern": "_"
          }
        ],
        "react/prop-types": 0
      },
      "settings": {
        "react": {
          "version": "detect"
        }
      }
    }
  ]
}
