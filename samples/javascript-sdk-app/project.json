{
  "projectType": "application",
  "root": "samples/javascript-sdk-app",
  "sourceRoot": "samples/javascript-sdk-app/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/web:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "samples/javascript-sdk-app/bundles",
        "index": "samples/javascript-sdk-app/src/index.html",
        "main": "packages/javascript-sdk/src/index.ts",
        "tsConfig": "samples/javascript-sdk-app/tsconfig.app.json",
        "webpackConfig": "samples/javascript-sdk-app/webpack.config.js"
      },
      "configurations": {
        "development": {
          "optimization": false,
          "watch": true
        },
        "production": {
          "optimized": true,
          "watch": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/web:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "javascript-sdk-app:build:development"
      },
      "configurations": {
        "development": {
          "buildTarget": "javascript-sdk-app:build:development"
        },
        "production": {
          "buildTarget": "javascript-sdk-app:build:production"
        }
      }
    }
  }
}
