{
  "name": "tests",
  "version": "1.0.0",
  "description": "E2E and integration tests for JS SDK",
  "devDependencies": {
    "@types/jest": "^25.2.3",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-dev-server": "^5.0.3",
    "jest-environment-jsdom": "^26.0.1",
    "playwright": "^1.0.2",
    "superagent": "^5.2.2",
    "ts-jest": "^26.5.3",
    "uuid": "^8.3.0"
  },
  "scripts": {
    "start:e2e": "ws -d e2e/app  -p 8443 --cors.origin null --cors.credentials --cert ../example.com+5.pem --key ../example.com+5-key.pem",
    "start:server": "node e2e/server/index.mjs",
    "start:server:live": "LIVE=true node e2e/server/index.mjs",
    "test:coverage": "jest --testMatch=\"<rootDir>/src/**/*.test.ts\" --config=./jest.coverage.config.js --env=\"<rootDir>/tests/jest.env.config.js\"",
    "test:e2e": "jest --testMatch=\"<rootDir>/tests/e2e/**/*.test.ts\" --config=./jest.e2e.config.js",
    "test:e2e:live": "LIVE=true jest --testMatch=\"<rootDir>/tests/e2e/**/*.lc.test.ts\" --config=./jest.e2e.config.js",
    "test:integration": "jest --testMatch=\"<rootDir>/tests/integration/**/*.test.ts\" --config=./jest.basic.config.js",
    "test:unit": "jest --testMatch=\"<rootDir>/src/**/*.test.ts\" --config=./jest.basic.config.js --env=\"<rootDir>/tests/jest.env.config.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cerebrl/forgerock-javascript-sdk.git"
  },
  "author": "",
  "license": "MIT",
  "homepage": "https://github.com/cerebrl/forgerock-javascript-sdk#readme"
}
