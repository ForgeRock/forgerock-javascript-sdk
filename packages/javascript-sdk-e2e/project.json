{
  "root": "packages/javascript-sdk-e2e",
  "sourceRoot": "packages/javascript-sdk-e2e/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/workspace:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/javascript-sdk-e2e",
        "main": "packages/javascript-sdk-e2e/src/index.ts",
        "tsConfig": "packages/javascript-sdk-e2e/tsconfig.lib.json",
        "assets": ["packages/javascript-sdk-e2e/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/javascript-sdk-e2e/**/*.ts"]
      }
    },
    "test-e2e": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/javascript-sdk-e2e"],
      "options": {
        "jestConfig": "packages/javascript-sdk-e2e/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
