{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/out-tsc",
    "declaration": true,
    "declarationMap": true,
    "noImplicitOverride": false,
    "noImplicitAny": false,
    "noPropertyAccessFromIndexSignature": false,
    "skipLibCheck": true,
    "lib": ["DOM", "WebWorker", "Webworker.Iterable"],
    "types": ["node"]
  },
  "include": ["src/**/*.ts"],
  "exclude": ["jest.config.ts", "src/**/*.spec.ts", "src/**/*.test.ts"]
}
