{
  "name": "@forgerock/javascript-sdk",
  "version": "4.4.2",
  "description": "ForgeRock JavaScript SDK",
  "author": "ForgeRock",
  "license": "MIT",
  "files": ["dist/*", "LICENSE", "README.md"],
  "module": "./dist/index.js",
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
