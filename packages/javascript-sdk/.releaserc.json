{
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "master",
    {
      "name": "develop",
      "prerelease": "beta"
    },
    {
      "name": "beta",
      "prerelease": true
    }
  ],
  "ci": true,
  "repositoryUrl": "https://github.com/ForgeRock/forgerock-javascript-sdk.git",
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    [
      "@semantic-release/changelog",
      {
        "changelogFile": "CHANGELOG.md"
      }
    ],
    [
      "@semantic-release/npm",
      {
        "npmPublish": true
      }
    ],
    [
      "@semantic-release/git",
      {
        "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}",
        "assets": ["lib-esm", "bundles", "lib", "../../!.gitignore", "package.json"]
      }
    ],
    [
      "@semantic-release/github",
      {
        "assets": ["lib-esm", "bundles", "lib", "../../!.gitignore"]
      }
    ]
  ]
}
