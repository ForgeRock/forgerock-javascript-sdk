{
  "ci": true,
  "commitMessage": "chore(release-${PROJECT_NAME}): ${nextRelease.version} [skip ci]",
  "gitAssets": [
    "packages/${PROJECT_NAME}/README.md",
    "packages/${PROJECT_NAME}/CHANGELOG.md"
  ],
  "buildTarget": "${PROJECT_NAME}:build",
  "outputPath": "dist/packages/${PROJECT_NAME}",
  "repositoryUrl": "https://github.com/ForgeRock/forgerock-javascript-sdk",
  "branches": [
    "+([0-9])?(.{+([0-9]),x}).x",
    "master",
    {
      "name": "develop",
      "prerelease": "beta"
    },
    {
      "name": "beta",
      "prerelease": true
    }
  ]
}
