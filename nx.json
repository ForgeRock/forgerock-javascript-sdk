{
  "npmScope": "forgerock",
  "targetDefaults": {
    "e2e": {
      "dependsOn": ["plugins-wait-for-api:build"]
    },
    "test:unit": {
      "dependsOn": ["^test"]
    },
    "test": {
      "dependsOn": ["^test"],
      "inputs": ["default", "^default"]
    },
    "tsc": {
      "dependsOn": ["^tsc"]
    },
    "build": {
      "dependsOn": ["^build"]
    },
    "prepare": {
      "dependsOn": ["^prepare"]
    },
    "package": {
      "dependsOn": ["^package"]
    },
    "lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"]
    }
  },
  "targetDependencies": {
    "test:unit": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "test": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "tsc": [
      {
        "target": "tsc",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "appsDir": "",
    "libsDir": ""
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx-cloud",
      "options": {
        "cacheableOperations": [
          "build",
          "build-interceptor",
          "build-types",
          "bundle-types",
          "test",
          "lint",
          "package",
          "prepare",
          "e2e",
          "version",
          "config",
          "tsc",
          "typedoc"
        ],
        "useDaemonProcess": true,
        "accessToken": "NzU3MWE3MjgtOWRiZi00ZDczLWI4YWYtMDI5NThmNTRkNDc1fHJlYWQtd3JpdGU="
      }
    }
  },
  "affected": {
    "defaultBase": "develop"
  },
  "cli": {
    "defaultCollection": "@nx/react"
  },
  "generators": {
    "@nx/web:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/web:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "css"
    },
    "@nx/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "javascript-sdk"
}
