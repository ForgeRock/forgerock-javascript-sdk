{
  "cliOptions": {
    "packageManager": "pnpm"
  },
  "targetDefaults": {
    "docs": {
      "cache": true
    },
    "e2e": {
      "cache": true
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true,
      "inputs": ["production", "^production"],
      "outputs": [
        "{workspaceRoot}/{projectRoot}/dist",
        "{workspaceRoot}/dist/{projectName}"
      ]
    },
    "build-interceptor": {
      "cache": true
    },
    "@nx/vite:test": {
      "dependsOn": ["^test"],
      "inputs": ["default", "^default"],
      "cache": true
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    },
    "@nx/vite:build": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["default", "^default"]
    },
    "@nx/esbuild:esbuild": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    }
  },
  "parallel": 1,
  "targetDependencies": {
    "test": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "appsDir": "",
    "libsDir": ""
  },
  "generators": {},
  "plugins": [
    {
      "plugin": "@nx/playwright/plugin",
      "options": {
        "ciTargetName": "e2e-ci",
        "targetName": "e2e"
      }
    },
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "lint"
      }
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "testTargetName": "test",
        "serveStaticTargetName": "serve-static"
      },
      "include": ["e2e/**/*", "packages/**/*"]
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "build-interceptor",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "testTargetName": "test",
        "serveStaticTargetName": "serve-static"
      },
      "include": ["e2e/token-vault-app/**/*"]
    },
    {
      "plugin": "@nx/vite/plugin",
      "options": {
        "buildTargetName": "package",
        "serveTargetName": "serve",
        "previewTargetName": "preview",
        "testTargetName": "test",
        "serveStaticTargetName": "serve-static"
      },
      "include": ["packages/javascript-sdk/**/*", "packages/ping-protect/**/*"]
    }
  ],
  "defaultProject": "javascript-sdk",
  "useDaemonProcess": true,
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "sharedGlobals": [
      "{workspaceRoot}/.github/workflows/ci.yml",
      "{workspaceRoot}/.github/workflows/merge.yml"
    ],
    "production": [
      "default",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json"
    ]
  },
  "useInferencePlugins": true,
  "defaultBase": "develop"
}
