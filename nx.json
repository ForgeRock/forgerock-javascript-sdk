{
  "targetDefaults": {
    "test": {
      "cache": true
    },
    "e2e": {
      "dependsOn": ["plugins-wait-for-api:build"],
      "cache": true
    },
    "tsc": {
      "dependsOn": ["^tsc"],
      "cache": true
    },
    "build": {
      "dependsOn": ["^build"],
      "cache": true
    },
    "build-interceptor": {
      "cache": true
    },
    "build-types": {
      "cache": true
    },
    "bundle-types": {
      "cache": true
    },
    "bundle": {
      "cache": true
    },
    "config": {
      "cache": true
    },
    "@nx/jest:jest": {
      "dependsOn": ["^test"],
      "inputs": ["default", "^default"],
      "cache": true,
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "@nx/eslint:lint": {
      "inputs": ["default", "{workspaceRoot}/.eslintrc.json"],
      "cache": true
    }
  },
  "targetDependencies": {
    "test:unit": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "test": [
      {
        "target": "test",
        "projects": "dependencies"
      }
    ],
    "tsc": [
      {
        "target": "tsc",
        "projects": "dependencies"
      }
    ],
    "build": [
      {
        "target": "build",
        "projects": "dependencies"
      }
    ],
    "prepare": [
      {
        "target": "prepare",
        "projects": "dependencies"
      }
    ],
    "package": [
      {
        "target": "package",
        "projects": "dependencies"
      }
    ]
  },
  "workspaceLayout": {
    "appsDir": "",
    "libsDir": ""
  },
  "affected": {
    "defaultBase": "develop"
  },
  "generators": {
    "@nx/web:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/web:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "none"
    },
    "@nx/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nx/angular:component": {
      "style": "css"
    },
    "@nx/react": {
      "application": {
        "style": "css",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "css"
      },
      "library": {
        "style": "css",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "javascript-sdk",
  "useDaemonProcess": true
}
