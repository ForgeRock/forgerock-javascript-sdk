{
  "name": "@forgerock/javascript-sdk",
  "version": "3.0.0",
  "description": "ForgeRock JavaScript SDK",
  "files": [
    "/bundles/**/*",
    "/lib/**/*",
    "/lib-esm/**/*",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "nx run javascript-sdk:build && nx run javascript-sdk:build:cjs",
    "build:analyze": "NODE_ENV=analyze nx run javascript-sdk:build --skipNxCache",
    "clean": "shx rm -rf ./docs ./coverage ./tmp",
    "clean:all": "npm run clean -ws && shx rm -rf samples/javascript-sdk-app/bundles ./node_modules ./tests/node_modules ./samples/reactjs-todo/node_modules ./samples/reactjs-todo/todos ./samples/reactjs-todo/users ./package-lock.json && git clean -fX -e \"!*.pem\" -e \"!.env\"",
    "docs": "typedoc",
    "format:staged": "pretty-quick --staged",
    "lint": "nx affected --base=develop --target=lint",
    "prepublishOnly": "npm run clean:all && npm i && npm run build && npm t",
    "start:server": "node e2e/javascript-sdk-app/src/server/index.mjs",
    "start:e2e": "nx run javascript-sdk-app:serve",
    "start:server:live": "LIVE=true node e2e/javascript-sdk-app/src/server/index.mjs",
    "start:samples": "ws -d samples -p 8443 --cors.origin null --cors.credentials --cert example.com+5.pem --key example.com+5-key.pem",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:e2e": "nx run e2e-javascript-sdk-app:e2e",
    "test:e2e:live": "nx run javascript-sdk-app:e2e --configuration=live -- LIVE=true",
    "test:integration": "nx affected --target=test --base=develop --configuration=integration",
    "test:unit": "nx affected --target=test --base=develop",
    "watch": "nx run javascript-sdk-app:build:development"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run clean -ws && npm run format:staged && npm run lint && npm run build",
      "pre-push": "npm t"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeRock/forgerock-javascript-sdk.git"
  },
  "author": "ForgeRock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForgeRock/forgerock-javascript-sdk/issues"
  },
  "homepage": "https://github.com/ForgeRock/forgerock-javascript-sdk#readme",
  "devDependencies": {
    "@nrwl/cli": "13.2.4",
    "@nrwl/eslint-plugin-nx": "13.2.4",
    "@nrwl/jest": "13.2.4",
    "@nrwl/linter": "13.2.4",
    "@nrwl/nx-cloud": "13.0.0",
    "@nrwl/react": "13.2.4",
    "@nrwl/tao": "13.2.4",
    "@nrwl/web": "13.2.4",
    "@nrwl/workspace": "13.2.4",
    "@ns3/nx-jest-playwright": "^1.3.0",
    "@playwright/test": "^1.17.1",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "^27.0.0",
    "@types/jest-dev-server": "^5.0.0",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@typescript-eslint/typescript-estree": "^4.7.0",
    "babel-jest": "27.2.3",
    "concurrently": "^6.4.0",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^9.0.0",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^4.2.5",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-dev-server": "^6.0.0",
    "jest-environment-jsdom": "^27.3.1",
    "jest-junit": "^12.0.0",
    "jest-playwright-preset": "^1.6.1",
    "local-web-server": "^4.2.1",
    "ngx-deploy-npm": "^3.0.5",
    "playwright": "^1.17.1",
    "prettier": "^2.1.1",
    "pretty-quick": "^3.1.2",
    "react-test-renderer": "17.0.2",
    "rollup-copy-plugin": "^0.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-visualizer": "^5.5.2",
    "shx": "^0.3.2",
    "superagent": "^5.2.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.17",
    "typedoc": "^0.17.7",
    "typescript": "^4.0.0",
    "util": "^0.12.3",
    "uuid": "^8.3.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.7.2"
  },
  "workspaces": [
    "samples/*",
    "packages/*",
    "e2e/*"
  ],
  "dependencies": {
    "core-js": "^3.19.2",
    "nx": "^13.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "rxjs": "^7.4.0",
    "tslib": "^2.0.0"
  }
}
