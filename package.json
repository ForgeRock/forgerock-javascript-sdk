    "eslint-plugin-prettier": "^5.1.3",
    "express": "^4.21.0",
    "fast-check": "^3.19.0",
    "husky": "^8.0.3",
    "jsdom": "22.1.0",
    "jsonc-eslint-parser": "^2.1.0",
    "lint-staged": "^13.2.2",
    "mkcert": "^3.2.0",
    "npm-cli-login": "^1.0.0",
    "nx": "20.1.2",
    "playwright": "^1.47.2",
    "prettier": "^3.2.5",
    "pretty-quick": "^4.0.0",
    "sass": "^1.71.1",
    "shx": "^0.3.4",
    "superagent": "^8.1.2",
    "swc-loader": "0.1.15",
    "ts-node": "10.9.1",
    "tslib": "^2.5.0",
    "typedoc": "^0.26.5",
    "typedoc-plugin-rename-defaults": "^0.7.1",
    "typedoc-theme-hierarchy": "^5.0.3",
    "typescript": "5.5.3",
