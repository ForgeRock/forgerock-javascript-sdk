{
  "name": "@forgerock/javascript-sdk",
  "version": "3.0.0",
  "description": "ForgeRock JavaScript SDK",
  "main": "./lib/",
  "module": "./lib-esm/",
  "files": [
    "/bundles/**/*",
    "/lib/**/*",
    "/lib-esm/**/*",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "npm run clean && nx run javascript-sdk:build -- --env DEV=no",
    "build:ci": "nx affected --target=build --base=remotes/origin/develop --parallel",
    "clean": "shx rm -rf ./bundles ./docs ./lib ./lib-esm ./coverage ./dist ./tmp",
    "clean:all": "npm run clean && shx rm -rf ./node_modules ./tests/node_modules ./samples/reactjs-todo/node_modules ./samples/reactjs-todo/todos ./samples/reactjs-todo/users ./package-lock.json && git clean -fX -e \"!*.pem\" -e \"!.env\"",
    "docs": "typedoc",
    "lint": "nx affected --base=develop --target=lint",
    "lint:ci": "nx affected --target=lint --base=remotes/origin/develop --parallel",
    "prepublishOnly": "npm run clean:all && npm i && npm run build && npm t",
    "start:e2e": "ws -d packages/javascript-sdk-e2e/src/e2e/app  -p 8443 --cors.origin null --cors.credentials --https",
    "start:server": "node packages/javascript-sdk-e2e/src/e2e/server/index.mjs",
    "start:server:live": "LIVE=true node packages/javascript-sdk/src/e2e/server/index.mjs",
    "start:samples": "ws -d samples -p 8443 --cors.origin null --cors.credentials --cert example.com+5.pem --key example.com+5-key.pem",
    "tsc": "nx affected --target=tsc --base=develop -- --verbose",
    "test": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:e2e": "jest --testMatch=\"<rootDir>/src/e2e/**/*.test.ts\" --config=\"./packages/javascript-sdk-e2e/jest.config.js\"",
    "test:e2e:live": "LIVE=true jest --testMatch=\"<rootDir>/tests/e2e/**/*.lc.test.ts\" --config=./tests/jest.e2e.config.js",
    "test:integration": "nx run javascript-sdk:test-integration",
    "test:integration:ci": "nx affected --target=test-integration --base=remotes/origin/develop",
    "test:unit": "nx run javascript-sdk:test-unit",
    "test:unit:ci": "nx affected --target=test-unit --base=remotes/origin/develop",
    "test:unit:affected": "nx affected --base=develop --target=test-unit",
    "watch": "nx run javascript-sdk:build -- --env DEV=yes"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run tsc && npm run lint",
      "pre-push": "npm run test:unit && npm run test:integration && npm run build && npm run test:e2e"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ForgeRock/forgerock-javascript-sdk.git"
  },
  "author": "ForgeRock",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ForgeRock/forgerock-javascript-sdk/issues"
  },
  "homepage": "https://github.com/ForgeRock/forgerock-javascript-sdk#readme",
  "devDependencies": {
    "@nrwl/cli": "13.2.0-rc.3",
    "@nrwl/eslint-plugin-nx": "13.2.0-rc.3",
    "@nrwl/jest": "13.2.0-rc.3",
    "@nrwl/linter": "13.2.0-rc.3",
    "@nrwl/nx-cloud": "12.5.2",
    "@nrwl/react": "13.2.0-rc.3",
    "@nrwl/tao": "13.2.0-rc.3",
    "@nrwl/web": "13.2.0-rc.3",
    "@nrwl/workspace": "13.2.0-rc.3",
    "@ns3/nx-jest-playwright": "^1.3.0",
    "@testing-library/react": "12.1.2",
    "@testing-library/react-hooks": "7.0.2",
    "@types/jest": "^27.0.0",
    "@types/react": "17.0.30",
    "@types/react-dom": "17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@typescript-eslint/typescript-estree": "^4.7.0",
    "babel-jest": "27.2.3",
    "cookie-parser": "^1.4.4",
    "copy-webpack-plugin": "^9.0.0",
    "copyfiles": "^2.1.1",
    "cors": "^2.8.5",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "7.26.1",
    "eslint-plugin-react-hooks": "4.2.0",
    "express": "^4.17.1",
    "husky": "^4.2.5",
    "jest": "^27.3.1",
    "jest-canvas-mock": "^2.2.0",
    "jest-dev-server": "^6.0.0",
    "jest-environment-jsdom": "^27.3.1",
    "jest-junit": "^12.0.0",
    "jest-playwright-preset": "^1.6.1",
    "local-web-server": "^4.2.1",
    "playwright": "^1.16.3",
    "prettier": "^2.1.1",
    "react-test-renderer": "17.0.2",
    "shx": "^0.3.2",
    "superagent": "^5.2.2",
    "ts-jest": "^27.0.7",
    "ts-loader": "^8.0.17",
    "typedoc": "^0.17.7",
    "typescript": "^4.0.0",
    "util": "^0.12.3",
    "uuid": "^8.3.0",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.7.2"
  },
  "workspaces": [
    "samples/*",
    "packages/*"
  ],
  "dependencies": {
    "core-js": "^3.6.5",
    "nx": "^13.1.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "regenerator-runtime": "0.13.7",
    "tslib": "^2.0.0"
  }
}
