{
  "version": 2,
  "projects": {
    "javascript-sdk": {
      "root": "packages/javascript-sdk",
      "sourceRoot": "packages/javascript-sdk/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "/packages/javascript-sdk/bundles/lib-esm/",
            "main": "packages/javascript-sdk/src/index.ts",
            "tsConfig": "packages/javascript-sdk/tsconfig.lib.json",
            "assets": ["packages/javascript-sdk/*.md"]
          }, 
          "configurations": {
            "commonjs": {
            "outputPath": "/packages/javascript-sdk/bundles/lib",
            "main": "packages/javascript-sdk/src/index.ts",
            "tsConfig": "packages/javascript-sdk/tsconfig.cjs.json",
            "assets": ["packages/javascript-sdk/*.md"]
            }
          }
        },
        "build:rollup": {
          "executor": "@nrwl/web:package",
          "options": {
            "entryFile": "packages/javascript-sdk/src/index.ts",
            "outputPath": "packages/javascript-sdk/bundles/",
            "project": "packages/javascript-sdk/package.json",
            "deleteOutputPath": true,
            "tsConfig": "packages/javascript-sdk/tsconfig.lib.json",
            "cwd": "packages/javascript-sdk",
            "parallel": true,
            "rollupConfig": "packages/javascript-sdk/rollup.config.js"
          },
          "outputs": ["{options.outputPath}"],
          "configurations": {
            "production": {},
            "development": {
              "watch": true
            }
          }
        },
        "build-webpack": {
          "executor": "@nrwl/workspace:run-commands",
          "options": {
            "commands": ["webpack --env DEV=no"],
            "cwd": "packages/javascript-sdk",
            "parallel": false
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/javascript-sdk/**/*.ts"],
            "ignorePath": ".gitignore"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/javascript-sdk"],
          "options": {
            "jestConfig": "packages/javascript-sdk/jest.unit.config.js",
            "passWithNoTests": true
          },
          "configurations": {
            "unit": {},
            "integration": {
              "jestConfig": "packages/javascript-sdk/jest.integration.config.js",
              "passWithNoTests": true
            }
          }
        },
        "deploy": {
          "executor": "ngx-deploy-npm:deploy",
          "options": {
            "access": "public",
            "buildTarget": "production",
            "dryRun": true,
            "noBuild": true
          },
          "configurations": {
            "alpha": {
              "tag": "alpha",
              "access": "public",
              "dryRun": true
            },
            "beta": {
              "tag": "alpha",
              "access": "public",
              "dryRun": true
            }
          }
        }
      },
      "tags": []
    },
    "javascript-sdk-e2e": {
      "root": "packages/javascript-sdk-e2e",
      "sourceRoot": "packages/javascript-sdk-e2e/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/workspace:tsc",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/packages/javascript-sdk-e2e",
            "main": "packages/javascript-sdk-e2e/src/index.ts",
            "tsConfig": "packages/javascript-sdk-e2e/tsconfig.lib.json",
            "assets": ["packages/javascript-sdk-e2e/*.md"]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": ["packages/javascript-sdk-e2e/**/*.ts"]
          }
        },
        "test-e2e": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/packages/javascript-sdk-e2e"],
          "options": {
            "jestConfig": "packages/javascript-sdk-e2e/jest.config.js",
            "passWithNoTests": true
          }
        }
      },
      "tags": []
    },
    "reactjs-todo": {
      "root": "samples/reactjs-todo",
      "projectType": "application",
      "targets": {
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/samples/reactjs-todo"],
          "options": {
            "jestConfig": "samples/reactjs-todo/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  }
}
